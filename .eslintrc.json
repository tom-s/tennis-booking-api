{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
        "indent": [2, 2, { "SwitchCase": 1 }],
        "semi": [2, "never"],
        "no-case-declarations": 0,
        "eol-last": 2,
        "no-cond-assign": 2,
        "no-constant-condition": 0,
        "object-curly-spacing": [2, "always"]
    }
}
